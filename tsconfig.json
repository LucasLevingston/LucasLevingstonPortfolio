{
	"compilerOptions": {
		"jsx": "react",

		"target": "ES2020" ,

		"module": "ES2020",
    "moduleResolution": "node",
"resolveJsonModule":true,
    "esModuleInterop": true ,
		"forceConsistentCasingInFileNames": true, 
		"strict": true ,
		"skipLibCheck": true, 
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
	},
	"include": ["src", "public", "typings", "vite-env.d.ts"]
}
